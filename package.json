{
  "name": "roospawn",
  "displayName": "RooSpawn",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onNotebook:roospawn"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "roospawn.helloWorld",
        "title": "RooSpawn: Hello World"
      }
    ],
    "notebooks": [
      {
        "type": "roospawn",
        "displayName": "RooSpawn Notebook",
        "selector": [
          {
            "filenamePattern": "*.dozer"
          }
        ]
      }
    ],
    "notebookRenderer": [
      {
        "id": "roospawn-status-renderer",
        "displayName": "RooSpawn Status Renderer",
        "entrypoint": "./dist-renderer/renderer.js",
        "mimeTypes": [
          "application/x-roospawn-status"
        ],
        "requiresMessaging": "always"
      }
    ]
  },
  "extensionDependencies": [
    "rooveterinaryinc.roo-cline"
  ],
  "scripts": {
    "vscode:prepublish": "yarn package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn compile-tests && yarn compile && yarn lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "download-pyodide": "node scripts/download-pyodide.js",
    "postinstall": "yarn download-pyodide"
  },
  "dependencies": {
    "react": "^19.0",
    "react-dom": "^19.0",
    "pyodide": "0.23.4",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/react": "^19.0",
    "@types/react-dom": "^19.0",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.97.0",
    "@types/vscode-notebook-renderer": "^1.72.3",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.19.0",
    "tar": "^7.4.3",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610",
  "files": [
    "dist",
    "dist-renderer",
    "resources"
  ]
}
